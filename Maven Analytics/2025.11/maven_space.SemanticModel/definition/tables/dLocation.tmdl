	column City = ```
			
			VAR vLocation = [Location]
			VAR vQtdVirgulas = LEN(vLocation) - LEN(SUBSTITUTE(vLocation, ",", ""))
			VAR vTextoSimples = TRIM(LEFT(vLocation, FIND(",", vLocation) - 1))
			VAR vTextoCompleto =
			    TRIM(
			        PATHITEMREVERSE(
			            SUBSTITUTE(vLocation, ",", "|"),
			            2
			        )
			    )
			RETURN
			    IF(vQtdVirgulas = 1, vTextoSimples, vTextoCompleto)
			
			```
		lineageTag: a676a5d4-358e-44e1-af5b-8fa62b76ec02
		summarizeBy: none

		annotation SummarizationSetBy = Automatic

	column Place = ```
			
			VAR vQtdVirgulas = LEN([Location]) - LEN(SUBSTITUTE([Location], ",", ""))
			VAR vTextoSimples = LEFT([Location], FIND(",", [Location]) - 1)
			VAR vTextv1 =
			    TRIM(
			        LEFT(
			            [Location],
			            LEN([Location])
			            - LEN(PATHITEMREVERSE(SUBSTITUTE([Location], ",", "|"), 2)) 
			            - LEN(PATHITEMREVERSE(SUBSTITUTE([Location], ",", "|"), 1)) 
			        )
			    )
			VAR vTextv2 =
			    IF(
			        RIGHT(vTextv1, 1) = ",",
			        LEFT(vTextv1, LEN(vTextv1) - 1),
			        vTextv1
			    )
			RETURN
			    IF(vQtdVirgulas = 1, vTextoSimples, vTextv2)
			
			```
